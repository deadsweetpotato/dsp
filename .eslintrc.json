{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "plugins": ["react", "react-hooks", "tailwindcss"],
  "rules": {
    "react/prop-types": "off",
    "tailwindcss/no-custom-classname": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
